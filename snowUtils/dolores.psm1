function Remove-LoremDump {
    $outputDirectory = $(Get-Location)
    $filesToDelete = Get-ChildItem -Path $outputDirectory -Filter "lorem_ipsum_*.md"

    foreach ($file in $filesToDelete) {
        Remove-Item -Path $file.FullName -Force
    }

    Write-Host "Markdown files cleaned up successfully from $outputDirectory"
}

